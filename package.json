{
  "name": "@tomasevich/tinkoff",
  "version": "0.5.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "docs": "typedoc",
    "build": "rm -rf ./dist && tsc",
    "test:instruments": "jest --testRegex='/test/InstrumentsService/*'",
    "test:marketdata": "jest --testRegex='/test/MarketDataService/*'",
    "test:operations": "jest --testRegex='/test/OperationsService/*'",
    "test:orders": "jest --testRegex='/test/OrdersService/*'",
    "test:sandbox": "jest --testRegex='/test/SandboxService/*'",
    "test:stoporders": "jest --testRegex='/test/StopOrdersService/*'",
    "test:users": "jest --testRegex='/test/UsersService/*'",
    "test": "npm run test:instruments && npm run test:marketdata && npm run test:operations && npm run test:orders && npm run test:sandbox && npm run test:stoporders && npm run test:users",
    "prerelease": "npm run build && npm test && npm run docs",
    "release": "npm publish --access=public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "dotenv": "^16.3.1",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasevich/tinkoff-invest-api.git"
  },
  "bugs": {
    "url": "https://github.com/tomasevich/tinkoff-invest-api/issues"
  },
  "homepage": "https://tomasevich.github.io/tinkoff",
  "description": "Неофициальная библиотека для работы с Тинькофф Инвестиции",
  "keywords": [
    "tinkoff-invest-api"
  ],
  "author": "Tomasevich V.S.",
  "license": "MIT"
}
